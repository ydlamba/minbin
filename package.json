    "url": "https://github.com/ydlamba/minbin/issues"
  },
  "homepage": "https://github.com/ydlamba/minbin#readme",
  "devDependencies": {
    "@types/node": "^10.3.3",
    "@types/react": "^16.3.18",
    "awesome-typescript-loader": "^5.0.0",
    "css-loader": "^0.28.11",
    "gh-pages": "^1.2.0",
    "html-webpack-plugin": "^3.2.0",
    "style-loader": "^0.21.0",
    "typescript": "^2.9.2",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "webpack-dev-server": "^3.1.4"
  },
  "dependencies": {
    "crypto-js": "^3.1.9-1",
